<mvc:View 
    controllerName="ui5.skills.controller.Detail"
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	>
  <Page
		title="{i18n>detailPageTitle}"
		showNavButton="true"
		navButtonPress=".onBackButtonPress"
		>
		<ObjectHeader intro="{skill>Skillarea}" 
		              title="{skill>Skillname}"/>	
	<core:Icon visible="true" 
	           src="sap-icon://area-chart"
			   size="100px"
			   >  
	</core:Icon>
  <content>	
    <Panel 
     class="sapUiContentPadding"
     width="auto"
	 height="auto">
     <content>
        <mvc:XMLView viewName="ui5.skills.view.RTE"/>
     </content>	 
    </Panel>	
   </content>    				  		  			  		  	  
  </Page>
</mvc:View>